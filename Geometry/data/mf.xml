  <LogicalPart name="ME2RingN" category="unspecified">
   <rSolid name="ME2Ring"/>
   <rMaterial name="materials:Air"/>
  </LogicalPart>
 </LogicalPartSection>

 <PosPartSection label="mf.xml">
  <PosPart copyNumber="1">
   <rParent name="muonBase:MUON"/>
   <rChild name="mf:MEP"/>
  </PosPart>
  <PosPart copyNumber="1">
   <rParent name="mf:MEP"/>
   <rChild name="mf:GE0RingP"/>
  </PosPart>
  <PosPart copyNumber="1">
   <rParent name="mf:MEP"/>
