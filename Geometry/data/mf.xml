 <LogicalPartSection label="mf.xml">
  <LogicalPart name="MEP" category="unspecified">
   <rSolid name="ME"/>
   <rMaterial name="materials:Air"/>
  </LogicalPart>
  <LogicalPart name="MEN" category="unspecified">
