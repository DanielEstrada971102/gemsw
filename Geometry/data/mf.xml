   <rMaterial name="materials:Air"/>
  </LogicalPart>
  <LogicalPart name="ME1RingP" category="unspecified">
   <rSolid name="ME1Ring"/>
   <rMaterial name="materials:Air"/>
  </LogicalPart>
  <LogicalPart name="ME1RingN" category="unspecified">
   <rSolid name="ME1Ring"/>
   <rMaterial name="materials:Air"/>
  </LogicalPart>
  <LogicalPart name="ME2RingP" category="unspecified">
   <rSolid name="ME2Ring"/>
